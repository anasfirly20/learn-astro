---
// data
import navData from "../data/navData";

// Miscellaneous
import { Icon } from "astro-icon";
---

<nav aria-label="Primary">
  <div class="navbar">
    <a href="/" class="logomark" aria-label="Go home">
      <Icon name="logomark" width="60" />
    </a>

    <ul>
      {navData.map((item) => <a href={item.path}>{item.name}</a>)}
    </ul>
  </div>
</nav>
